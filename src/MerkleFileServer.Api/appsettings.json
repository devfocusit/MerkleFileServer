{
  "MemoryStorage": {
    "AbsoluteExpirationInMinutes": 120
  },

  /* Below is used with client addon when simulating distributed peers.
     Endpoints shall be configured to specific scenario (ports numbers). 
     Peers can also be passed within request body. 
     Pointing trusted TrustedServer is most importnat here for that scenario
  */
  "MerkleFileClient": {
    "TrustedServer": "https://localhost:7000",
    "Peers": [
      "https://localhost:7001",
      "https://localhost:7002"
    ]
  }
}
